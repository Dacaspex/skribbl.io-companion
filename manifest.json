{
  "name": "Scribbl.io Companion",
  "version": "1.0",
  "description": "Companion for scribbl.io",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://skribbl.io/*"
      ],
      "css": [
        "src/app.css"
      ],
      "js": [
        "lib/jquery.js",
        "src/app.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/app.css"
  ]
}